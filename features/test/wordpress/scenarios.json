{
    "alpine-base": {
        "image": "ghcr.io/automattic/vip-codespaces/alpine-base:latest",
        "overrideCommand": false,
        "features": {
            "php": {},
            "mariadb": {},
            "wp-cli": {},
            "wordpress": {},
            "nginx": {}
        },
        "overrideFeatureInstallOrder": [
            "./php",
            "./nginx",
            "./wp-cli",
            "./mariadb",
            "./wordpress"
        ]
    },
    "alpine-base-persist": {
        "image": "ghcr.io/automattic/vip-codespaces/alpine-base:latest",
        "overrideCommand": false,
        "features": {
            "php": {},
            "mariadb": {
                "installDatabaseToWorkspaces": true
            },
            "wp-cli": {},
            "wordpress": {
                "moveUploadsToWorkspaces": true
            },
            "nginx": {}
        },
        "overrideFeatureInstallOrder": [
            "./php",
            "./nginx",
            "./wp-cli",
            "./mariadb",
            "./wordpress"
        ]
    },
