{
    "id": "ssh",
    "name": "SSH",
    "description": "Sets up SSH into the Dev Environment",
    "version": "1.2.0",
    "documentationURL": "https://github.com/Automattic/vip-codespaces/tree/trunk/features/src/ssh",
    "options": {
        "enabled": {
            "type": "boolean",
            "default": true,
            "description": "Enable OpenSSH"
        },
        "port": {
            "type": "string",
            "default": "22",
            "description": "Port to expose for SSH"
        },
        "new_password": {
            "type": "string",
            "default": "skip",
            "description": "New password for the user",
            "proposals": ["skip", "random"]
        }
    }
}
